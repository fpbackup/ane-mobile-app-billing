<extension xmlns="http://ns.adobe.com/air/extension/18.0">

    <id>com.funkypanda.mobile-billing</id>

    <versionNumber>0.0.1</versionNumber>

    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libmobile-billing-ios.a</nativeLibrary>
                <initializer>InAppPurchaseInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libmobile-billing-ios.a</nativeLibrary>
                <initializer>InAppPurchaseInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>lib-billing-android.jar</nativeLibrary>
                <initializer>com.funkypanda.mobilebilling.Extension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>